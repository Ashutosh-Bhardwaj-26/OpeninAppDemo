<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView2"
            android:elevation="15dp"
            app:cardCornerRadius="10dp"
            android:layout_margin="5dp"
            android:shadowColor="@color/black"
            android:padding="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <RelativeLayout
                android:id="@+id/frame_10"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:background="@drawable/frame_10">

                <RelativeLayout
                    android:id="@+id/frame_14"
                    android:layout_height="96dp"
                    android:layout_width="95dp"
                    android:layout_alignParentStart="true"
                    android:layout_marginStart="12dp"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="12dp">

                    <RelativeLayout
                        android:id="@+id/software"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_alignParentStart="true"
                        android:layout_marginStart="6dp"
                        android:layout_alignParentTop="true"
                        android:layout_marginTop="6dp"
                        />

                    <RelativeLayout
                        android:id="@+id/frame_11"
                        android:layout_marginTop="16dp"
                        android:layout_below="@+id/software"
                        android:layout_width="wrap_content"
                        android:layout_height="48dp">
                        <TextView
                            android:id="@+id/some_id"
                            android:layout_width="wrap_content"
                            android:layout_height="24dp"
                            android:textAppearance="@style/some_id"
                            android:lineSpacingExtra="5sp"
                            android:gravity="center_vertical"
                            />

                        <TextView
                            android:id="@+id/today_s_cli"
                            android:layout_below="@id/some_id"
                            android:layout_width="wrap_content"
                            android:layout_height="20dp"
                            android:text="@string/today_s_cli"
                            android:textAppearance="@style/today_s_cli"
                            android:lineSpacingExtra="3sp"
                            android:gravity="center_vertical"
                            />
                    </RelativeLayout>
                </RelativeLayout>
            </RelativeLayout>
        </androidx.cardview.widget.CardView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>